<main>
  <div class='errorDiv'>
  <p>{{error}}</p>
  <p>{{msg}}</p>
  </div>

  <div id='dashboardDiv'>
  <h1 id='dashH1'>Browse Games</h1>
  <hr>
  <br>
    <br>

<dl>
  <div class='gamesInfo'>

  {{#each game}}
  <div class='singleGame'>
    <a href='/games/{{this._id}}'>
      <img alt= {{this.name}} src='{{this.artwork}}' class='gameName'/>
    </a>
  </div>
  {{/each}}
</dl>
</div>
</main>
